{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug momo_cli",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/app/cli/momo_cli",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb/lldb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: build"
    }
  ]
}




